  "license": "MIT",
  "bugs": {
    "url": "https://github.com/soruly/trace.moe-telegram-bot/issues"
  },
  "homepage": "https://github.com/soruly/trace.moe-telegram-bot",
  "devDependencies": {
    "@effect-ak/tg-bot-api": "^1.0.0",
    "@types/node": "^25.1.0",
    "prettier": "^3.8.1"
  },
  "dependencies": {
    "postgres": "^3.4.8"
  }
